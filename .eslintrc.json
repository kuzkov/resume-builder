{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["xo", "xo-react/space", "xo-typescript", "plugin:react/jsx-runtime", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": "error"
  }
}
